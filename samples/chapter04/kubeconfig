apiVersion: v1
kind: Config
preferences: {}
clusters: # 接続先クラスタ
  - name: sample-cluster
    cluster:
      server: https://localhost:6443
users: # 認証情報
  - name: sample-user
    user:
      client-certificate-data: LS0tLS1CRUdJTi...
      client-key-data: LS0tLS1CRUdJTi...
contexts: # 接続先と認証情報の組み合わせ
  - name: sample-context
    context:
      cluster: sample-cluster
      namespace: default
      user: sample-user
current-context: sample-context

